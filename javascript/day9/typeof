const person = {
  name: '홍길동',
  age: 30,
  blood: 'A',
  gender: '남',
};

//typeof 자료형 확인

console.log(typeof 1);
console.log(typeof '');
console.log(typeof []);
console.log(typeof {});
console.log(typeof function () {});
console.log(typeof null);
console.log(typeof undefiled);
console.log(typeof Symbol());
console.log(typeof person);